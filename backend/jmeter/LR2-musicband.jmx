            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="script">import groovy.json.JsonSlurper
def json = new JsonSlurper().parseText(new String(prev.getResponseData()))
def seen = [] as Set
json.content.each { b -&gt;
    def key = b.name + "|" + b.genre
    if(!seen.add(key)) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage("Duplicate name+genre: "+key)
    }
}
</stringProp>
